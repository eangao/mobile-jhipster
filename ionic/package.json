{
  "name": "ionic",
  "version": "0.0.1",
  "private": true,
  "description": "A hipster Ionic project, made with ðŸ’™ by @oktadev!",
  "homepage": "https://www.jhipster.tech",
  "author": "Ionic Framework + JHipster",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e --port 8100",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --write \"{,e2e/**/,src/**/}*.{js,json,html,md,ts,css,scss,yml}\" --loglevel silent"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/**/*.{js,ts}",
      "!src/app/**/index.ts",
      "!src/app/**/*.module.ts",
      "!<rootDir>/node_modules/"
    ],
    "coverageDirectory": "./public/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/e2e/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^@ionic/storage$": "<rootDir>/node_modules/@ionic/storage/dist/ionic-storage.cjs.js"
    },
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/src/setupTests.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "src/test.ts"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|@ionic-native|@ionic|ionic-appauth|capacitor-secure-storage-plugin)"
    ]
  },
  "dependencies": {
    "@angular/common": "~11.2.0",
    "@angular/core": "~11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/router": "~11.2.0",
    "@fortawesome/angular-fontawesome": "0.8.2",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@ionic-native/camera": "5.32.1",
    "@ionic-native/core": "5.32.1",
    "@ionic-native/http": "5.33.0",
    "@ionic-native/secure-storage": "5.33.0",
    "@ionic-native/splash-screen": "5.32.1",
    "@ionic-native/status-bar": "5.32.1",
    "@ionic/angular": "^5.5.2",
    "@ionic/storage": "^3.0.4",
    "@ionic/storage-angular": "^3.0.6",
    "@ng-bootstrap/ng-bootstrap": "9.1.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "cordova-plugin-advanced-http": "3.1.0",
    "cordova-plugin-file": "6.0.2",
    "cordova-plugin-safariviewcontroller": "2.0.0",
    "cordova-plugin-secure-storage-echo": "5.1.1",
    "ionic-appauth": "0.7.4",
    "ngx-webstorage": "7.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.4",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "~11.2.4",
    "@angular/compiler": "~11.2.0",
    "@angular/compiler-cli": "~11.2.0",
    "@angular/language-service": "~11.2.0",
    "@angular/localize": "^11.2.14",
    "@ionic/angular-toolkit": "^3.1.1",
    "@oktadev/schematics": "^4.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "26.0.23",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "generator-jhipster": "6.10.5",
    "generator-jhipster-ionic": "7.0.0",
    "html-loader": "2.1.2",
    "ionic-mocks-jest": "1.3.3",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "26.6.3",
    "jest-localstorage-mock": "2.4.10",
    "jest-preset-angular": "8.4.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "null-loader": "4.0.1",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "ts-loader": "9.1.1",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-camera": {
        "CAMERA_USAGE_DESCRIPTION": "Need camera access to take pictures",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": "Need photo library access to get pictures from there"
      }
    }
  }
}
